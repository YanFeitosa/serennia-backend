{
  "name": "serenna-backend",
  "version": "1.0.0",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:clean": "node scripts/kill-port-and-dev.js",
    "dev:safe": "node scripts/kill-port-and-dev.js",
    "build": "tsc",
    "start": "node dist/index.js",
    "kill:port": "powershell -ExecutionPolicy Bypass -File scripts/kill-port.ps1",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "node scripts/migrate-deploy.js",
    "prisma:db:push": "node scripts/db-push.js",
    "prisma:apply-manual-migrations": "node scripts/apply-manual-migrations.js",
    "prisma:generate": "prisma generate",
    "create-users": "ts-node src/scripts/createInitialUsers.ts",
    "create-super-admin": "ts-node src/scripts/createSuperAdmin.ts",
    "run-migration": "ts-node src/scripts/runMigration.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@supabase/supabase-js": "^2.84.0",
    "bcrypt": "^6.0.0",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.14.0",
    "prisma": "^6.19.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
